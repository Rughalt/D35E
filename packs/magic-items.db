{"_id":"d16sug8RYT91mNfF","name":"Silver Dagger, Masterwork","type":"weapon","img":"systems/D35E/icons/items/magic/weapons/named/silver-dagger-masterwork.png","data":{"originVersion":97,"description":{"value":"<p><strong>Silver Dagger, Masterwork:</strong>&nbsp;This masterwork alchemical silver dagger is nonmagical. As a masterwork weapon, it has a +1 enhancement bonus on attack rolls.</p>\n<p>No aura (nonmagical); Price 322 gp.</p>","chat":"","unidentified":""},"nameFormula":"","nameFromFormula":false,"tags":[],"uniqueId":"","source":"","classSource":"","userNonRemovable":false,"customAttributes":{},"customAttributesLocked":false,"customTag":"","createdBy":"","quantity":1,"weight":1,"constantWeight":true,"price":322,"identified":true,"hp":{"max":2,"value":2},"hardness":10,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"Silver Dagger, Masterwork","container":"None","containerId":"none","containerWeightless":false,"equippedWeightless":false,"changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"noInt":false,"noCon":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false,"multiAttack":false,"multiweaponAttack":false,"uncannyDodge":false},"conditionFlags":{"dazzled":false,"wildshaped":false,"polymorphed":false},"combatChanges":[],"combatChangesRange":{"maxFormula":"","max":0},"combatChangesAdditionalRanges":{"hasAdditionalRanges":false,"slider1":{"maxFormula":"","max":0,"name":""},"slider2":{"maxFormula":"","max":0,"name":""},"slider3":{"maxFormula":"","max":0,"name":""}},"combatChangesUsesCost":"chargesPerUse","combatChangesApplySpecialActionsOnce":true,"combatChangeCustomDisplayName":"","combatChangeCustomReferenceName":"","sizeOverride":"","counterName":"","resistances":[],"damageReduction":[],"requirements":[],"creationChanges":[],"contextNotes":[],"enhancements":{"uses":{"value":0,"max":0,"per":null,"rechargeFormula":null,"autoDeductCharges":true,"