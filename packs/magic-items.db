{"_id":"u3JMLhA5yYmnTLyw","name":"Cloak of Elvenkind","type":"equipment","img":"systems/D35E/icons/items/magic/cloak/cloak_16.png","data":{"originVersion":97,"description":{"value":"\n      <div topic='Cloak of Elvenkind' level='8'><p><b>Cloak of Elvenkind:</b> This cloak of neutral gray cloth is indistinguishable from an ordinary cloak of the same color. However, when worn with the hood drawn up around the head, it gives the wearer a +5 competence bonus on Hide checks.</p><p>Faint illusion; CL 3rd; Craft Wondrous Item, <i>invisibility</i>,  creator must be an elf; Price 2,500 gp; Weight 1 lb.</p>\n</div>\n    ","chat":"","unidentified":""},"nameFormula":"","nameFromFormula":false,"tags":[],"uniqueId":"","source":"","classSource":"","userNonRemovable":false,"customAttributes":{},"customAttributesLocked":false,"customTag":"","createdBy":"","quantity":1,"weight":1,"constantWeight":true,"price":2500,"identified":true,"hp":{"max":10,"value":10},"hardness":0,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"Cloak of Elvenkind","container":"None","containerId":"none","containerWeightless":false,"equippedWeightless":false,"changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"noInt":false,"noCon":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false,"multiAttack":false,"multiweaponAttack":false,"uncannyDodge":false},"conditionFlags":{"dazzled":false,"wildshaped":false,"polymorphed":false},"combatChanges":[],"combatChangesRange":{"maxFormula":"","max":0},"combatChangesAdditionalRanges":{"hasAdditionalRanges":false,"slider1":{"maxFormula":"","max":0,"name":""},"slider2":{"maxFormula":"","max":0,"name":""},"slider3":{"maxFormula":"","max":0,"name":""}},"combatChangesUsesCost":"chargesPerUse","combatChangesApplySpecialActionsOnce":true,"combatChangeCustomDisplayName":"","combatChangeCustomReferenceName":"","sizeOverride":"","counterName":"","resistances":[],"da